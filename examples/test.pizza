base test(x) (1+2+x)*(x+(1+2));

test(4);
test(5);

sauce print(x);

print(test(5));

base fib(x)
  if x < 3 then
    1
  else
    fib(x-1)+fib(x-2);

fib(1);
fib(6);

sauce printchar(char);

base printstar(n)
  for i = 1, i < n, 1.0 in
    printchar(42);  # ascii 42 = '*'

# print 100 '*' characters
printstar(100);
